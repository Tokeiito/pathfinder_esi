{
  "name": "tokeiito/pathfinder_esi",
  "description": "ESI API library for Pathfinder",
  "type": "library",
  "homepage": "https://github.com/tokeiito/pathfinder_esi",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "Sam O'Neill",
      "email": "samuel.p.oneill@gmail.com"
    }
  ],
  "config": {
    "optimize-autoloader": true,
    "lock": false
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/tokeiito/pathfinder_esi"
    }
  ],
  "autoload": {
    "psr-4": {
      "Exodus4D\\ESI\\": "app/"
    }
  },
  "require": {
    "php-64bit": ">=8",
    "ext-json": "*",
    "guzzlehttp/guzzle": "7.9.*",
    "caseyamcl/guzzle_retry_middleware": "2.12.*",
    "cache/void-adapter": "1.2.*"
  }
}
